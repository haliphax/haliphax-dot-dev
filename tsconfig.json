{
  "exclude": ["tasks/"],
  "include": [".eleventy.ts", "**/*.ts"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "rootDir": "."
  }
}
